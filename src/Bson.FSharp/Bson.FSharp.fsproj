<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Bson.FSharp</PackageId>
    <Version>1.0.5-alpha</Version>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Enrique Alfonso Casanovas Pedre</Authors>
    <Description>This library contains some serializers to make the use of BSON more friendly in F#</Description>
    <PackageProjectUrl>https://github.com/eacasanovaspedre/Bson.FSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/eacasanovaspedre/Bson.FSharp</RepositoryUrl>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Serializers.fs" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>
